server 10.8.0.0 255.255.255.0
port 1194
proto udp
dev tun

ca ca.crt
cert server.crt
key server.key
dh dh.pem

ifconfig-pool-persist ipp.txt

push "redirect-gateway def1 bypass-dhcp"
push "dhcp-option DNS 192.168.100.1"

keepalive 10 120

--data-ciphers AES-256-GCM:AES-128-GCM

persist-key
persist-tun
status openvpn-status.log
verb 3

client-to-client